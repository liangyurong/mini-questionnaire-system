<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lyr.qs.mapper.SurveyMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lyr.qs.entity.Survey">
        <id column="id" property="id" />
        <result column="title" property="title" />
        <result column="description" property="description" />
        <result column="create_time" property="createTime" />
        <result column="survey_state" property="surveyState" />
        <result column="survey_question_num" property="surveyQuestionNum" />
        <result column="answer_num" property="answerNum" />
        <result column="visibility" property="visibility" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, title, description, create_time, survey_state, survey_question_num, answer_num, visibility
    </sql>

</mapper>
